@model List<Findergers1._0.Models.ViewModels.ListDesa>

@{
    ViewBag.Title = "Desaparecidos";
}

<h2>@ViewBag.Title</h2>

<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>#</th>
                <th>Name</th>
                <th>Age</th>
                <th>Description</th>
                <th>Average</th>
                <th>Image</th>
                <th> </th>
            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.Id</td>
                    <td>@oElemento.Nombre</td>
                    <td>@oElemento.Edad</td>
                    <td>@oElemento.Descripcion</td>
                    <td>@oElemento.FechaDesaparicion</td>
                    <td>@oElemento.Imagen</td>
                    <td>
                        <a class="btn btn-default" href="~/Desaparecidos/Edit/@oElemento.Id">Edit</a>
                        <a class="btn btn-danger" href="~/Desaparecidos/Delete/@oElemento.Id">Delete</a>

                    </td>
                </tr>
            }

        </table>

    </div>
</div>

<tr>
    <td>
        <a href="/Desaparecidos/NewDesa" class="btn btn-primary">Nuevo Registro</a>
    </td>
</tr>


